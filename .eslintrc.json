{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": [
            "error", { "argsIgnorePattern": "^_" }
        ],
        "eqeqeq": "error",
        "no-extra-bind": "error"
    }
}
